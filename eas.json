{
  "cli": {
    "version": ">= 3.6.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m1-medium"
      },
      "android": {
        "buildType": "apk",
        "env": {
          "EXPO_TARGET_SDK_VERSION": "34"
        },
        "image" : "latest"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m1-medium"
      },
      "android": {
        "buildType": "apk",
        "env": {
          "EXPO_TARGET_SDK_VERSION": "34"
        },
        "image" : "latest"
      }
    },
    "production": {
      "ios": {
        "resourceClass": "m1-medium"
      },
      "android": {
        "buildType": "app-bundle",
        "env": {
          "EXPO_TARGET_SDK_VERSION": "34"
        },
        "image" : "latest"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
